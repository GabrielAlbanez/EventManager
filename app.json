{
  "expo": {
    "name": "my-expo-app",
    "slug": "native-py",
    "version": "1.0.0",
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": [
      [
        "expo-system-ui",
        {
          "androidNavigationBar": {
            "visible": false,
            "barStyle": "light-content"
          },
          "androidStatusBar": {
            "visible": false,
            "barStyle": "light-content"
          }
        }
      ],
      [
        "expo-image-picker",
        {
          "cameraPermission": "Permitir que o aplicativo acesse sua c√¢mera."
        }
      ],
      [
        "@rnmapbox/maps",
        {
          "RNMapboxMapsDownloadToken": "sk.eyJ1IjoiZ2FicmllbGxiYW5leiIsImEiOiJjbWEyaW5ocmQwMnk3Mm1vanNkYzc2eHUyIn0.-GA737cnF8RW6kchU4Em6Q",
          "RNMapboxMapsVersion": "11.0.0"
        }
      ],
      [
        "expo-location",
        {
          "LocationWhenInUsePermission": "Show current location on map."
        }
      ]
    ],

    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.gabrielalbanez.eventmanager.ios"
    },
    "android": {
      "package": "com.gabrielalbanez.eventmanager.android",

      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "extra": {
      "eas": {
        "projectId": "12e1f1d5-9071-4741-b0bc-63419a0cdf21"
      }
    },
    "owner": "gabrel_albanez",
    "androidStatusBar": {
      "hidden": true,
      "translucent": true
    }
  }
}
